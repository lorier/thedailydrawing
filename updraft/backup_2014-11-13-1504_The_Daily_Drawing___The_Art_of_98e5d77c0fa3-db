# End of data contents of table `tddnet_posts`

# Table: `tddnet_term_relationships`
# Approximate rows expected in table: 519

# Delete any existing table `tddnet_term_relationships`

DROP TABLE IF EXISTS `tddnet_term_relationships`;

# Table structure of table `tddnet_term_relationships`

CREATE TABLE `tddnet_term_relationships` (
  `object_id` bigint(20) unsigned NOT NULL DEFAULT '0',
  `term_taxonomy_id` bigint(20) unsigned NOT NULL DEFAULT '0',
  `term_order` int(11) NOT NULL DEFAULT '0',
  PRIMARY KEY (`object_id`,`term_taxonomy_id`),
  KEY `term_taxonomy_id` (`term_taxonomy_id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 ;

# Data contents of table `tddnet_term_relationships`

 
